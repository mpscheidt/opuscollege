	<subDataset name="Dataset_pauta" uuid="58a4682c-cb78-4934-a319-1b0c395b7d6f">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Esura"/>
		<parameter name="subjectId" class="java.lang.Integer"/>
		<parameter name="attempt" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT DISTINCT
	     student.studentid AS student_studentid,
	     student.studentcode AS student_studentcode,
	     person.surnamefull AS person_surnamefull,
	     person.firstnamesfull AS person_firstnamesfull,
	     person.firstnamesfull || ' ' || person.surnamefull AS person_fullname,
	     opuscollege.remove_diacritics(person.firstnamesfull || ' ' || person.surnamefull) AS person_fullname_nodiacritics,

	     'E' || Examination.id AS assessment_id,
	     '1' || '-' || Examination.examinationdescription AS "assessment_description",
	    examinationresult.mark AS assessment_mark,
	    examinationresult.passed AS passed

		 FROM opuscollege.person person
	     INNER JOIN opuscollege.student student ON person.id = student.personid
	     INNER JOIN opuscollege.studyplan studyplan ON student.studentid = studyplan.studentid
	     INNER JOIN opuscollege.studyPlanCardinalTimeUnit ON studyplancardinaltimeunit.studyPlanId = studyplan.id
	     INNER JOIN opuscollege.studyplandetail ON studyplandetail.studyplancardinaltimeunitid = studyPlanCardinalTimeUnit.id
	     
	     inner JOIN opuscollege.ExaminationResult ON Examinationresult.studyplandetailid = studyplandetail.id
	     inner JOIN opuscollege.Examination ON Examination.Id = ExaminationResult.ExaminationId
	     
	     inner JOIN opuscollege.ExaminationResult exameFinalResult ON exameFinalResult.studyplandetailid = studyplandetail.id
	     inner JOIN opuscollege.Examination exameFinal ON exameFinal.Id = exameFinalResult.ExaminationId and exameFinal.examinationtypecode = '201'
	     
	     WHERE examination.subjectId =$P{subjectId}
	         and examination.examinationtypecode = '200'
	         and exameFinal.subjectId =$P{subjectId}
	          and exameFinalResult.attemptnr =  3
	          and ExaminationResult.passed = 'Y'
	         

UNION

SELECT DISTINCT
	     student.studentid AS student_studentid,
	     student.studentcode AS student_studentcode,
	     person.surnamefull AS person_surnamefull,
	     person.firstnamesfull AS person_firstnamesfull,
	     person.firstnamesfull || ' ' || person.surnamefull AS person_fullname,
	     opuscollege.remove_diacritics(person.firstnamesfull || ' ' || person.surnamefull) AS person_fullname_nodiacritics,

	     'E' || Examination.id AS assessment_id,
	     '2' || '-' || '25% Freq' AS "assessment_description",
	    CAST(round(examinationresult.mark::numeric * 0.25,2) AS VARCHAR) AS assessment_mark,
	    examinationresult.passed AS passed

		 FROM opuscollege.person person
	     INNER JOIN opuscollege.student student ON person.id = student.personid
	     INNER JOIN opuscollege.studyplan studyplan ON student.studentid = studyplan.studentid
	     INNER JOIN opuscollege.studyPlanCardinalTimeUnit ON studyplancardinaltimeunit.studyPlanId = studyplan.id
	     INNER JOIN opuscollege.studyplandetail ON studyplandetail.studyplancardinaltimeunitid = studyPlanCardinalTimeUnit.id
	     
	     inner JOIN opuscollege.ExaminationResult ON Examinationresult.studyplandetailid = studyplandetail.id
	     inner JOIN opuscollege.Examination ON Examination.Id = ExaminationResult.ExaminationId
	     
	     inner JOIN opuscollege.ExaminationResult exameFinalResult ON exameFinalResult.studyplandetailid = studyplandetail.id
	     inner JOIN opuscollege.Examination exameFinal ON exameFinal.Id = exameFinalResult.ExaminationId and exameFinal.examinationtypecode = '201'
	     
	     WHERE examination.subjectId =$P{subjectId}
	         and examination.examinationtypecode = '200'
	         and exameFinal.subjectId =$P{subjectId}
	          and exameFinalResult.attemptnr =  3
	          and ExaminationResult.passed = 'Y'
	          
UNION

SELECT DISTINCT
	     student.studentid AS student_studentid,
	     student.studentcode AS student_studentcode,
	     person.surnamefull AS person_surnamefull,
	     person.firstnamesfull AS person_firstnamesfull,
	     person.firstnamesfull || ' ' || person.surnamefull AS person_fullname,
	     opuscollege.remove_diacritics(person.firstnamesfull || ' ' || person.surnamefull) AS person_fullname_nodiacritics,

	     'E' || Examination.id AS assessment_id,
	     '3' || '-' || exameFinal.examinationdescription || ' Externo' AS "assessment_description",
	    examefinalresult.mark AS assessment_mark,
	    examefinalresult.passed AS passed

		 FROM opuscollege.person person
	     INNER JOIN opuscollege.student student ON person.id = student.personid
	     INNER JOIN opuscollege.studyplan studyplan ON student.studentid = studyplan.studentid
	     INNER JOIN opuscollege.studyPlanCardinalTimeUnit ON studyplancardinaltimeunit.studyPlanId = studyplan.id
	     INNER JOIN opuscollege.studyplandetail ON studyplandetail.studyplancardinaltimeunitid = studyPlanCardinalTimeUnit.id
	     
	    inner JOIN opuscollege.ExaminationResult ON Examinationresult.studyplandetailid = studyplandetail.id and examinationResult.passed='Y'
	     inner JOIN opuscollege.Examination ON Examination.Id = ExaminationResult.ExaminationId  and Examination.examinationtypecode = '200'
	     
	     inner JOIN opuscollege.ExaminationResult exameFinalResult ON exameFinalResult.studyplandetailid = studyplandetail.id and exameFinalResult.attemptnr =  3
	     inner JOIN opuscollege.Examination exameFinal ON exameFinal.Id = exameFinalResult.ExaminationId and exameFinal.examinationtypecode = '201' 
	     
	     WHERE examination.subjectId =$P{subjectId}
	     				and exameFinal.subjectId =$P{subjectId}
	        
			
			
UNION

SELECT DISTINCT
	     student.studentid AS student_studentid,
	     student.studentcode AS student_studentcode,
	     person.surnamefull AS person_surnamefull,
	     person.firstnamesfull AS person_firstnamesfull,
	     person.firstnamesfull || ' ' || person.surnamefull AS person_fullname,
	     opuscollege.remove_diacritics(person.firstnamesfull || ' ' || person.surnamefull) AS person_fullname_nodiacritics,

	     'E' || exameFinal.id AS assessment_id,
	     '4' || '-' || '75% Exame Externo' AS "assessment_description",
	    CAST(round(exameFinalResult.mark::numeric * 0.75,2) AS VARCHAR) AS assessment_mark,
	    exameFinalResult.passed AS passed

		 FROM opuscollege.person person
	     INNER JOIN opuscollege.student student ON person.id = student.personid
	     INNER JOIN opuscollege.studyplan studyplan ON student.studentid = studyplan.studentid
	     INNER JOIN opuscollege.studyPlanCardinalTimeUnit ON studyplancardinaltimeunit.studyPlanId = studyplan.id
	     INNER JOIN opuscollege.studyplandetail ON studyplandetail.studyplancardinaltimeunitid = studyPlanCardinalTimeUnit.id
	     
	    inner JOIN opuscollege.ExaminationResult ON Examinationresult.studyplandetailid = studyplandetail.id and examinationResult.passed='Y'
	     inner JOIN opuscollege.Examination ON Examination.Id = ExaminationResult.ExaminationId and Examination.examinationtypecode = '200'
	       inner JOIN opuscollege.ExaminationResult exameFinalResult ON exameFinalResult.studyplandetailid = studyplandetail.id and exameFinalResult.attemptnr =  3
	     inner JOIN opuscollege.Examination exameFinal ON exameFinal.Id = exameFinalResult.ExaminationId and exameFinal.examinationtypecode = '201' 
	     
	    
	     WHERE examination.subjectId =$P{subjectId} 
	     				and exameFinal.subjectId =$P{subjectId}
	         
			
			

UNION

SELECT DISTINCT
	     student.studentid AS student_studentid,
	     student.studentcode AS student_studentcode,
	     person.surnamefull AS person_surnamefull,
	     person.firstnamesfull AS person_firstnamesfull,
	     person.firstnamesfull || ' ' || person.surnamefull AS person_fullname,
	     opuscollege.remove_diacritics(person.firstnamesfull || ' ' || person.surnamefull) AS person_fullname_nodiacritics,

	     'S' || subjectResult.subjectId AS assessment_id,
	     '5' || '-' || 'Nota Final' AS "assessment_description",
	     
	     CASE
		   WHEN  exameFinalResult.mark::numeric < 8.0 THEN null
		   WHEN  exameFinalResult.mark::numeric >= 8.0 THEN subjectResult.mark
	     END AS assessment_mark,
	     subjectResult.passed AS passed

		 FROM opuscollege.person person
	     INNER JOIN opuscollege.student student ON person.id = student.personid
	     INNER JOIN opuscollege.studyplan studyplan ON student.studentid = studyplan.studentid
	     INNER JOIN opuscollege.studyPlanCardinalTimeUnit ON studyplancardinaltimeunit.studyPlanId = studyplan.id
	     INNER JOIN opuscollege.studyplandetail ON studyplandetail.studyplancardinaltimeunitid = studyPlanCardinalTimeUnit.id

	     inner JOIN opuscollege.subjectResult ON subjectResult.studyplandetailid = studyplandetail.id
	         AND subjectResult.subjectId =$P{subjectId}
	         
	     inner JOIN opuscollege.ExaminationResult ON Examinationresult.studyplandetailid = studyplandetail.id
	     inner JOIN opuscollege.Examination ON Examination.Id = ExaminationResult.ExaminationId
	     
	     inner JOIN opuscollege.ExaminationResult exameFinalResult ON exameFinalResult.studyplandetailid = studyplandetail.id  and exameFinalResult.attemptnr =  3
	     inner JOIN opuscollege.Examination exameFinal ON exameFinal.Id = exameFinalResult.ExaminationId and exameFinal.examinationtypecode = '201'
	     WHERE exameFinal.subjectId =$P{subjectId}
	         and Examination.examinationtypecode = '200'
			and examinationResult.passed='Y'
	        

UNION

SELECT DISTINCT
	     student.studentid AS student_studentid,
	     student.studentcode AS student_studentcode,
	     person.surnamefull AS person_surnamefull,
	     person.firstnamesfull AS person_firstnamesfull,
	     person.firstnamesfull || ' ' || person.surnamefull AS person_fullname,
	     opuscollege.remove_diacritics(person.firstnamesfull || ' ' || person.surnamefull) AS person_fullname_nodiacritics,
	     'S' || recorrencia.subjectId AS assessment_id,
	     '6' || '-' || 'Resultado' AS "assessment_description",
	     CASE
		   WHEN  recorrenciaResult.mark::numeric < 8.0 THEN 'Reprovado *'
		   WHEN  subjectResult.passed = 'Y' THEN 'Aprovado'
		   WHEN  subjectResult.passed = 'N' THEN 'Reprovado'
	     END AS assessment_mark,
	    notafreqResult.passed AS passed

		 FROM opuscollege.person person
	     INNER JOIN opuscollege.student student ON person.id = student.personid
	     INNER JOIN opuscollege.studyplan studyplan ON student.studentid = studyplan.studentid
	     INNER JOIN opuscollege.studyPlanCardinalTimeUnit ON studyplancardinaltimeunit.studyPlanId = studyplan.id
	     INNER JOIN opuscollege.studyplandetail ON studyplandetail.studyplancardinaltimeunitid = studyPlanCardinalTimeUnit.id
	     
	     left outer JOIN opuscollege.subjectResult ON subjectResult.studyplandetailid = studyplandetail.id
	         AND subjectResult.subjectId =$P{subjectId}

	     inner JOIN opuscollege.ExaminationResult recorrenciaResult ON recorrenciaResult.studyplandetailid = studyplandetail.id and recorrenciaResult.attemptnr =  3
	     inner JOIN opuscollege.Examination recorrencia ON recorrencia.Id = recorrenciaResult.ExaminationId 
	     
	     inner JOIN opuscollege.ExaminationResult notaFreqResult ON notaFreqResult.studyplandetailid = studyplandetail.id and notaFreqResult.passed='Y'
	     inner JOIN opuscollege.Examination notaFreq ON notaFreq.Id = notaFreqResult.ExaminationId and notaFreq.examinationtypecode = '200'
	     
	     inner JOIN opuscollege.ExaminationResult exameFinalResult ON exameFinalResult.studyplandetailid = studyplandetail.id
	     inner JOIN opuscollege.Examination exameFinal ON exameFinal.Id = exameFinalResult.ExaminationId and exameFinal.examinationtypecode = '201'
	     
	    
	     WHERE recorrencia.subjectId =$P{subjectId} and exameFinal.subjectId =$P{subjectId}
	        
	     order by person_fullname_nodiacritics, assessment_description]]>
		</queryString>
