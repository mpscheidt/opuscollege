	<queryString>
		<![CDATA[SELECT
    institution.institutiondescription AS institution_description,
    branch.branchdescription AS branch_description,
    organizationalunit.organizationalunitdescription AS organizationalunit_organizationalunitdescription,
    primarystudy.studydescription AS primarystudy_studydescription,
	cardinaltimeunit.description AS cardinaltimeunit_Description,
	studytime.description AS studytime_description,
	gradetype.description AS gradetype_description,
	
	subjectblockstudygradetype.cardinaltimeunitnumber AS subjectblockstudygradetype_cardinaltimeunitnumber,
	
	studyplancardinaltimeunit.cardinaltimeunitnumber AS studyplancardinaltimeunit_cardinaltimeunitnumber, 
	academicyear.description as academicyear_description,
    subject.subjectcode,
	subject.subjectdescription,
	subject.creditamount,
	address.city AS address_city,
	subject.id as subject_id

FROM opuscollege.subject
    INNER JOIN opuscollege.academicyear ON academicyear.id = subject.currentacademicyearid
    INNER JOIN opuscollege.study primarystudy ON subject.primarystudyid = primarystudy.id
    
    INNER JOIN  opuscollege.subjectsubjectblock ON subjectsubjectblock.subjectid=subject.id
    INNER JOIN  opuscollege.subjectblock ON subjectsubjectblock.subjectblockid=subjectblock.id
    INNER JOIN  opuscollege.subjectblockstudygradetype ON subjectblockstudygradetype.subjectblockid=subjectblock.id
    
    INNER JOIN opuscollege.subjectteacher ON subjectteacher.subjectid=subject.id
    INNER JOIN opuscollege.staffmember ON staffmember.staffmemberid =subjectteacher.staffmemberid
    INNER JOIN opuscollege.person ON person.id =staffmember.personid
    INNER JOIN opuscollege.studygradetype ON primarystudy.id = studygradetype.studyid
    INNER JOIN opuscollege.cardinaltimeunit ON studygradetype.cardinaltimeunitcode = cardinaltimeunit.code AND cardinaltimeunit.lang = $P{lang}
    INNER JOIN opuscollege.studytime ON subjectblock.studytimecode = studytime.code AND studytime.lang = $P{lang}
    INNER JOIN opuscollege.studyplancardinaltimeunit ON studygradetype.id = studyplancardinaltimeunit.studygradetypeid
      INNER JOIN opuscollege.gradetype gradetype ON studygradetype.gradetypecode = gradetype.code AND gradetype.lang = $P{lang}
  
    INNER JOIN opuscollege.organizationalunit ON primarystudy.organizationalunitid = organizationalunit.id
    INNER JOIN opuscollege.branch ON organizationalunit.branchid = branch.id
    INNER JOIN opuscollege.institution ON branch.institutionid = institution.id
    
	     LEFT OUTER JOIN opuscollege.address ON (address.organizationalunitid = organizationalunit.id) AND (address.addresstypecode = '5')	
	     LEFT OUTER JOIN "opuscollege"."country" country ON address."countrycode" = country."code" AND country.lang = $P{lang}
WHERE 1=1
 $P!{whereClause}]]>
	</queryString>
